<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<title>Hello world!</title>
	<script src="modernizr.js"></script>
	<script type="text/javascript">
	window.addEventListener("load",eventWindowLoaded,false);
	function eventWindowLoaded(){
		var theCanvas = document.getElementById("canvasX");
		var c = theCanvas.getContext("2d");

		c.fillStyle = "#ffffaa";
		c.fillRect(0,0,500,300);
		c.beginPath();
		c.strokeStyle = "black";
		c.lineWidth = 5;
		c.arc(100,100,20,(Math.PI/180)*0,(Math.PI/180)*360,false);
		c.stroke();
		c.closePath();
		console.log(theCanvas.toDataURL())
	}
</script>
</head>
<body>
<canvas id="canvasX" width="500" height="300">You browser does not support HTML Canvas.</canvas>
</body>

</html>
